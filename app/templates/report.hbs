<CompanyStyles @metadata={{@model.metadata}} />
<ThemeWrapper @theme={{theme}}>
  <div class="poster-frame">
    <header class="header">
      <HeaderLogo @logoUrl={{@model.metadata.logotype}} @alt={{@model.metadata.name}} />
      <h1 class="header__title">
        {{@model.metadata.name}}
        <span>Â·</span> Recruitment highlights
      </h1>
      <BasicDropdown @renderInPlace="true" as |dd|>
        <dd.Trigger class="header__share">
          <svg
            width="24"
            height="24"
            viewBox="0 0 24 24"
            xmlns="http://www.w3.org/2000/svg"
            fill-rule="evenodd"
            clip-rule="evenodd"
          >
            <path d="M16 2v7h-2v-5h-12v16h12v-5h2v7h-16v-20h16zm2 9v-4l6 5-6 5v-4h-10v-2h10z"></path>
          </svg>
        </dd.Trigger>
        <dd.Content>
          {{#fb-share-button}}Facebook{{/fb-share-button}}
          {{#linkedin-share-button}}Linkedin{{/linkedin-share-button}}
        </dd.Content>
      </BasicDropdown>
    </header>
    <div class="page-wrapper">
      <section class="cover">
        <h2 class="cover__headline u-text-shadow a__zoomin">
          2019
          <span class="u-text-shadow--small u-box-shadow">
            Â· Year in review Â·
          </span>
          <lottie-player
            class="a__lottie--poppers"
            src="/assets/lottie/jumpers.json"
            background="transparent"
            speed="1"
            style="width: 300px; height: 300px;"
            loop
            autoplay
          ></lottie-player>
        </h2>
        <a href="#numbers" class="cover__directions">
          <svg xmlns="http://www.w3.org/2000/svg" width="34" height="34" viewBox="0 0 24 24">
            <path d="M0 12l9-8v6h15v4h-15v6z"></path>
          </svg>
          This way
        </a>
        <button class="cover__switch" {{action "handleThemeChange"}}>
          <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24">
            <path
              d="M0 12c0 6.627 5.373 12 12 12s12-5.373 12-12-5.373-12-12-12-12 5.373-12 12zm2 0c0-5.292 4.113-9.612 9.315-9.965-3.205 2.154-5.315 5.813-5.315 9.965s2.11 7.811 5.315 9.965c-5.202-.353-9.315-4.673-9.315-9.965z"
            ></path>
          </svg>
        </button>
      </section>
      <section class="numbers" id="numbers">
        <div class="number number--visitors">
          <span class="intro">
            Our
            <strong>
              career site
            </strong>
            <span class="u-nowrap">had over</span>
          </span>
          <span class="number__item">
            {{round @model.visits.count}}
          </span>
          visitors browsing
        </div>
        <div class="number number--candidate-type">
          <div class="number__wrapper">
            <span class="number__item">
              {{@model.sourced.count}}
              <strong>
                Sourced
              </strong>
            </span>
            <span class="number__item">
              {{@model.connected.count}}
              <strong>
                Connected
              </strong>
            </span>
            <span class="number__item">
              {{@model.applied.count}}
              <strong>
                Applied
              </strong>
            </span>
          </div>
        </div>
        {{#if (gt @model.hires.count 0)}}
          <div class="number number--hires">
            <span class="number__item">
              <Counter @value={{@model.hires.count}} />
            </span>
            <svg class="heart" xmlns="http://www.w3.org/2000/svg" width="400" height="400" viewBox="0 0 24 24">
              <path
                d="M12 4.248c-3.148-5.402-12-3.825-12 2.944 0 4.661 5.571 9.427 12 15.808 6.43-6.381 12-11.147 12-15.808 0-6.792-8.875-8.306-12-2.944z"
              ></path>
            </svg>
            <span class="intro">
              <strong>
                Hires
              </strong>
            </span>
            {{#if (gt @model.daysToHire.count 0)}}
              <div class="number number--days-to-hire">
                <span>
                  Average days
                </span>
                to hire
                <strong>
                  {{@model.daysToHire.count}}
                </strong>
              </div>
            {{/if}}
          </div>
        {{/if}}
        <div class="collaborate">
          <div class="number number--employees">
            <span class="intro">
              We have
            </span>
            <span class="number__item">
              <Counter @value={{@model.activeEmployees.count}} />
            </span>
            <strong>
              Active employees
            </strong>
            in Teamtailor
          </div>
          <div class="number number--activities">
            <span class="intro">
              And
            </span>
            <span class="number__item">
              {{round @model.activities.count}}
            </span>
            <strong>
              Collaborative activities
            </strong>
            such as interviews, reviews and candidate communication
          </div>
          {{#if (eq @theme "light")}}
            <lottie-player
              class="a__lottie--party-penguin"
              src="/assets/lottie/swing-light.json"
              background="transparent"
              speed="1"
              style="width: 150px; height: 150px;"
              loop
              autoplay
            ></lottie-player>
          {{else}}
            <lottie-player
              class="a__lottie--party-penguin"
              src="/assets/lottie/swing-dark.json"
              background="transparent"
              speed="1"
              style="width: 150px; height: 150px;"
              loop
              autoplay
            ></lottie-player>
          {{/if}}
        </div>
        {{#if (gt @model.tshirts.count 0)}}
          <div class="number number--shirts">
            <span class="number__item">
              {{@model.tshirts.count}}
            </span>
            <span class="intro">
              <strong>
                T-shirts earned
                <a href="#footnote" class="footnote">
                  *
                </a>
              </strong>
            </span>
            <svg
              width="400"
              height="400"
              viewBox="0 0 24 24"
              xmlns="http://www.w3.org/2000/svg"
              fill-rule="evenodd"
              clip-rule="evenodd"
            >
              <path
                d="M19 24h-14v-16l-2 1.084-3-3.612 6-5.472h12l6 5.505-3 3.647-2-1.152v16zm-11-23h8c-.365 1.846-1.792 3.5-4 3.5s-3.635-1.708-4-3.5z"
              ></path>
            </svg>
          </div>
        {{/if}}
      </section>
      <section class="join-us">
        <div class="open-positions">
          <div class="open-positions__wrapper">
            <span class="open-positions__number">
              {{@model.openJobs.count}}
            </span>
            <strong>
              Open positions
            </strong>
          </div>
        </div>
        <h3 class="join-us__title">
          Want to join our team?
          <lottie-player
            class="a__lottie--happy-blob"
            src="/assets/lottie/happy.json"
            background="transparent"
            speed="1"
            style="width: 300px; height: 300px;"
            loop
            autoplay
          ></lottie-player>
        </h3>
        <a href={{@model.metadata.url}} class="join-us__link">
          Visit our career site
        </a>
      </section>
